    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^8.12.0",
    "@types/node": "^24.6.2",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "axios": "^0.30.2",
    "buffer": "^6.0.3",
    "cookie": "^1.0.2",
    "dayjs": "^1.11.13",
    "openid-client": "^6.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.9.3",
