{
  "name": "web-waku",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "waku dev",
    "build": "waku build",
    "start": "waku start",
    "build:lambda": "rm -rf dist && waku build --with-aws-lambda",
    "ladle": "ladle serve",
    "test:visual": "npx playwright test",
    "test:update": "npx playwright test -u",
    "test:docker": "docker compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "test:docker-update": "docker compose -f docker-compose.test.yml run --rm web-waku-test npm run test:update",
    "test:headed": "npx playwright test --headed",
    "test:ui": "npx playwright test --ui"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-server-dom-webpack": "19.1.1",
    "waku": "0.26.1"
  },
  "devDependencies": {
    "@ladle/react": "^5.0.3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/vite": "4.1.13",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@types/sync-fetch": "^0.4.3",
    "sync-fetch": "^0.6.0-2",
    "tailwindcss": "4.1.13",
    "typescript": "5.9.2"
  }
}
